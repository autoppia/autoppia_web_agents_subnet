# Autoppia Miner Agent Configuration

# Required: Agent identification
AGENT_NAME=My Custom Agent
AGENT_VERSION=1.0.0
GITHUB_URL=https://github.com/your-username/your-repo

# Optional: Agent capabilities
HAS_RL=false  # Set to "true" if using reinforcement learning
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR

# Optional: Performance tuning
MAX_EXECUTION_TIME=30  # Maximum task execution time in seconds
MAX_ACTIONS=50  # Maximum number of actions per task

# Optional: Web driver configuration
WEBDRIVER_TIMEOUT=10  # WebDriver timeout in seconds
HEADLESS=true  # Run browser in headless mode

# Optional: API configuration
API_TIMEOUT=30  # API request timeout in seconds
RETRY_ATTEMPTS=3  # Number of retry attempts for failed requests
